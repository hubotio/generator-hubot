{
  "name": "<%= appname %>",
  "description": "<%= scriptDescription %>",
  "version": "0.0.0",
  "keywords": "<%= scriptKeywords %>",
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= orgName %>/<%= appname %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= orgName %>/<%= appname %>/issues"
  },
  "dependencies": {
    "bluebird": "^3.3.4"
  },
  "peerDependencies": {
    "hubot": "2.x"
  },
  "devDependencies": {
    "hubot-test-helper": "github:eedevops/hubot-test-helper",
    "hubot-enterprise": "github:eedevops/hubot-enterprise",
    "chai": "^2.1.1",
    "coffee-script": "1.6.3",
    "hubot": "2.x",
    "jshint": "^2.9.2",
    "coffeelint": "^1.15.7",
    "jenkins-mocha": "^2.6.0"
  },
  "main": "index.coffee",
  "scripts": {
    "coffeelint": "coffeelint --reporter jslint -r . > coffee-lint.xml || true",
    "jslint": "jshint --exclude-path .gitignore --reporter=jslint .> js-lint.xml || true",
    "test": "ARTIFACTS_DIR='./' COVERAGE_DIR='./' TEST_DIR='./test/' && jenkins-mocha --compilers \"coffee:coffee-script/register\" --check-leaks --no-coverage test/**/*.coffee || true",
    "lint": "coffeelint -r .; jshint --exclude-path .gitignore . || true"
  },
  "coffeelintConfig": {
    "max_line_length": {
      "limitComments": false
    }
  },
  "author": "<%= scriptOwner %>",
  "license": "Apache-2.0"
}
